{
    "version": "0.2.0",
    "configurations": [{
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/.electron-vue/dev-runner.js",
            "name": "Launch via NPM",
            "runtimeExecutable": "node",
            "sourceMaps": true,
            "port": 5858,
            "env": {
                "NODE_ENV": "development"
            }        
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}