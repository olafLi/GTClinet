{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                // "program": "${workspaceRoot}/.electron-vue/dev-runner.js",
                "name": "Launch via NPM",
                "runtimeExecutable": "cnpm",
                "runtimeArgs": ["run","dev"],
                "port": 5858,
                "env": {
                    "NODE_ENV":"development"
                }
            },
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${workspaceRoot}/dist/electron/main.js"
            }
        ]
    }